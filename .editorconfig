# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.py]
charset = utf-8
indent_style = space
indent_size = 4
max_line_length = 160

[*.yml]
indent_style = space
indent_size = 2
trim_trailing_whitespace = True